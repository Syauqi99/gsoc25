{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-debug",
            "type": "shell",
            "command": "g++",
            "args": [
                "-g",
                "-std=c++14",
                "-I${workspaceFolder}/include",
                "-I/usr/include/eigen3",
                "${workspaceFolder}/scripts/test3_main.cpp",
                "${workspaceFolder}/src/InteriorPointLP.cpp",
                "-o",
                "${workspaceFolder}/bin/test3_debug"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "problemMatcher": [
                "$gcc"
            ],
            "presentation": {
                "echo": true,
                "reveal": "always",
                "focus": false,
                "panel": "shared",
                "showReuseMessage": true,
                "clear": false
            }
        }
    ]
}
